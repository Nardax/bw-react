{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/images/*.*"
      
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "ordered-imports": false,
    "prefer-const": false,
    "variable-name": false,
    "no-debugger": false
  }
}